<Window x:Class="ReportCardGenerator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReportCardGenerator"
        mc:Ignorable="d"
        Title="ReportCard" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Height="24" Margin="10" VerticalAlignment="Top"  FontSize="20" Text="Report Card Generator" />

        <Button x:Name="generateButton" Content="Add!" HorizontalAlignment="Left" Margin="339,379,0,0" VerticalAlignment="Top" Click="Button_Click" ClickMode="Release"/>
        <Label x:Name="fName" Content="First Name" HorizontalAlignment="Left" Margin="134,85,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lName" Content="Last Name" HorizontalAlignment="Left" Margin="134,116,0,0" VerticalAlignment="Top"/>


    </Grid>
</Window>
